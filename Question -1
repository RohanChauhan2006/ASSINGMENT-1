// Create a function that accepts multiple numbers and returns their sum using the rest operator.

import React from 'react';

function SumComponent({ numbers }) {
  const sumNumbers = (...nums) => nums.reduce((total, num) => total + num, 0);

  return (
    <div>
      <h1>Sum Calculator</h1>
      <p>The sum of the numbers is: {sumNumbers(...numbers)}</p>
    </div>
  );
}


